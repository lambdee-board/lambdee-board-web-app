{
  "eslint.format.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "cSpell.words": [
    "camelcase",
    "Cyclomatic",
    "dataurl",
    "dotfiles",
    "esbuild",
    "fontawesome",
    "fortawesome",
    "Guardfile",
    "jdenticon",
    "Lambdee",
    "linebreak",
    "livereload",
    "multilines",
    "nonblock",
    "noscript",
    "outfile",
    "paren",
    "parens",
    "recents",
    "reduxjs",
    "stylesheet"
  ],
  "files.associations": {
    "*.yml.example": "yaml",
    "*.env.example": "env",
    "*.js.erb": "javascript",
    "Procfile*": "procfile"
},
}