{
  "eslint.format.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.tabSize": 2,
  "cSpell.words": [
    "bgcolor",
    "camelcase",
    "Cyclomatic",
    "dataurl",
    "datetime",
    "dotfiles",
    "esbuild",
    "fontawesome",
    "fortawesome",
    "Guardfile",
    "jdenticon",
    "jsonapi",
    "Lambdee",
    "linebreak",
    "livereload",
    "multilines",
    "nonblock",
    "noscript",
    "openapi",
    "outfile",
    "paren",
    "parens",
    "plpgsql",
    "recents",
    "reduxjs",
    "Rswag",
    "stylesheet"
],
  "files.associations": {
    "*.yml.example": "yaml",
    "*.env.example": "env",
    "*.js.erb": "javascript",
    "Procfile*": "procfile"
  },
  "[ruby]": {
    "editor.tabSize": 2
  },
  "[css.erb]": {
      "editor.tabSize": 2
  },
  "[erb]": {
      "editor.tabSize": 2
  },
  "[html.erb]": {
      "editor.tabSize": 2
  },
  "[html]": {
      "editor.tabSize": 2
  },
  "[haml]": {
      "editor.tabSize": 2
  },
  "[js.erb]": {
      "editor.tabSize": 2
  },
  "[js]": {
      "editor.tabSize": 2
  },
  "[json]": {
      "editor.tabSize": 2
  },
  "[yml]": {
      "editor.tabSize": 2
  },
  "[yaml]": {
      "editor.tabSize": 2
  },
  "[scss]": {
      "editor.tabSize": 2
  },
  "[sass]": {
      "editor.tabSize": 2
  },
  "[scss.erb]": {
      "editor.tabSize": 2
  },
}