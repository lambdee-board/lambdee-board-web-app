{
  "eslint.format.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.tabSize": 2,
  "cSpell.words": [
    "ansicolor",
    "Archivable",
    "authenticatable",
    "bgcolor",
    "camelcase",
    "clike",
    "Cyclomatic",
    "dataurl",
    "datetime",
    "dotfiles",
    "esbuild",
    "fira",
    "fontawesome",
    "fontsource",
    "fortawesome",
    "Guardfile",
    "hoverable",
    "inputmode",
    "jbuilder",
    "jdenticon",
    "jsonapi",
    "katex",
    "Lambdee",
    "linebreak",
    "livereload",
    "monospace",
    "msgpack",
    "msgpackr",
    "multilines",
    "nonblock",
    "noscript",
    "nowrap",
    "omniauthable",
    "openapi",
    "outfile",
    "paren",
    "parens",
    "plpgsql",
    "precompiled",
    "prismjs",
    "procs",
    "recents",
    "reduxjs",
    "rehype",
    "Rswag",
    "sortablejs",
    "stylesheet",
    "TASKLIST",
    "timeoutable",
    "TTFB",
    "unnasigned",
    "validatable",
    "Xmark"
],
  "files.associations": {
    "*.yml.example": "yaml",
    "*.env.example": "env",
    "*.js.erb": "javascript",
    "Procfile*": "procfile"
  },
  "[ruby]": {
    "editor.tabSize": 2
  },
  "[css.erb]": {
      "editor.tabSize": 2
  },
  "[erb]": {
      "editor.tabSize": 2
  },
  "[html.erb]": {
      "editor.tabSize": 2
  },
  "[html]": {
      "editor.tabSize": 2
  },
  "[haml]": {
      "editor.tabSize": 2
  },
  "[js.erb]": {
      "editor.tabSize": 2
  },
  "[js]": {
      "editor.tabSize": 2
  },
  "[json]": {
      "editor.tabSize": 2
  },
  "[yml]": {
      "editor.tabSize": 2
  },
  "[yaml]": {
      "editor.tabSize": 2
  },
  "[scss]": {
      "editor.tabSize": 2
  },
  "[sass]": {
      "editor.tabSize": 2
  },
  "[scss.erb]": {
      "editor.tabSize": 2
},
"workbench.colorCustomizations": {
    "activityBar.activeBackground": "#5a84af",
    "activityBar.activeBorder": "#e0bfcf",
    "activityBar.background": "#5a84af",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#e0bfcf",
    "activityBarBadge.foreground": "#15202b",
    "sash.hoverBorder": "#5a84af",
    "statusBar.background": "#466b90",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#5a84af",
    "statusBarItem.remoteBackground": "#466b90",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#466b90",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#466b9099",
    "titleBar.inactiveForeground": "#e7e7e799"
},
